{
  "name": "@libraui/extension-packager",
  "version": "0.2.1",
  "description": "MDF components extension packager",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": "./lib/cli.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags && npm run build && ynpm publish",
    "build": "rm -rf lib && tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:quiet": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --quiet --fix",
    "lint:git": "git diff --cached --name-only --diff-filter=ACM | grep '\\.[tj]sx\\?$' | xargs eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:git",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@git.yonyou.com:mdf-5.0/extension.git"
  },
  "files": [
    "README.md",
    "package.json",
    "lib"
  ],
  "author": "Yudu Ban <banyd@yonyou.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@libraui/extension": "^0.5.0",
    "@types/autoprefixer": "^9.6.1",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.3",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.13",
    "@types/webpack": "^4.41.0",
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.7.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.1.0",
    "tsc": "^1.20150623.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.7.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "css-loader": "^3.2.0",
    "ignore-map-loader": "0.0.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "source-map-loader": "^0.2.4",
    "ts-import-plugin": "^1.6.1",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.2",
    "yargs": "^15.0.2"
  }
}
